<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="23" defaultlocale="pt-PT" id="com.cordova.splinvent" version="0.1.100" windows-packageVersion="1.1.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>SplInvent</name>
    <description>A simple aplication.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Luis Silva</author>
    <content src="index.html" />
    <access origin="*" />
    <vs:features />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget id="com.cordova.splinvent" platformName="windows" version="0.0.1.0">
            <vs:appxPackageIsForStore>False</vs:appxPackageIsForStore>
            <vs:appxBundle>Auto</vs:appxBundle>
            <vs:appxBundlePlatforms>x86</vs:appxBundlePlatforms>
            <vs:packageOutputPath>C:\Dev\Projectos\SplInvent\AppPackages\</vs:packageOutputPath>
        </vs:platformSpecificWidget>
    </vs:platformSpecificValues>
